<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         id="product"
         name="%productName"
         point="org.eclipse.core.runtime.products">
      <product
            application="org.eclipse.ui.ide.workbench"
            name="Punten IDE Development Tools">
         <property
               name="preferenceCustomization"
               value="plugin_customization.ini">
         </property>
         <property
               name="appName"
               value="Punten IDE Development Tools">
         </property>
         <property
               name="aboutText"
               value="%productDescription">
         </property>
      </product>
   </extension>

</plugin>
